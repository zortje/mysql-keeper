{
  "name": "zortje/mysql-keeper",
  "description": "Find incorrect configurations or missed optimization opportunities in MySQL databases",
  "version": "0.4.0",
  "type": "library",
  "keywords": ["mysql", "optimization"],
  "license": "MIT",
  "authors": [
    {
      "name": "René Tønner Stokholm",
      "email": "zortje@gmail.com",
      "homepage": "http://www.renestokholm.dk",
      "role": "developer"
    }
  ],
  "support": {
    "issues": "https://github.com/zortje/mysql-keeper/issues"
  },
  "require": {
    "php": ">=5.4",
    "ext-PDO": "~1.0",
    "ext-pdo_mysql": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Zortje\\MySQLKeeper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zortje\\MySQLKeeper\\Tests\\": "tests/"
    }
  }
}
